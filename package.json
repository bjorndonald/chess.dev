{
  "name": "chess.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "chess:build": "bun --watch build ./chess/index.ts --outdir ./dist",
    "chess:style": "npx tailwindcss -i ./chess/styles/style.css -o ./dist/output.css --watch",
    "chess:run": "http-server ./dist -p 5500",
    "lint": "eslint 'src/{app,components}/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/{app,components}/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --check \"src/{components,app}/**/*.{js,jsx,ts,tsx,css,scss,json,yaml,md}\"",
    "format:fix": "prettier --write \"src/{components,app}/**/*.{js,jsx,ts,tsx,css,scss,json,yaml,md}\"",
    "types:check": "tsc --pretty --noEmit",
    "migration:generate": "drizzle-kit generate",
    "migration:push": "NODE_TLS_REJECT_UNAUTHORIZED='0' drizzle-kit push",
    "lint-staged": {
      "**/*": "prettier --write --ignore-unknown"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@tailwindcss/typography": "^0.5.13",
    "@types/moment": "^2.13.0",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "contentlayer": "^0.3.4",
    "daisyui": "^4.11.1",
    "drizzle-kit": "^0.23.0",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.13.2",
    "rehype-slug": "^6.0.0",
    "sass": "^1.77.8",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.4.2",
    "@hookform/resolvers": "^3.9.0",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "chess.js": "^1.0.0-beta.8",
    "classnames": "^2.5.1",
    "drizzle-orm": "^0.32.0",
    "framer-motion": "^11.2.0",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "next": "^14.2.3",
    "next-auth": "^5.0.0-beta.18",
    "nextjs-toploader": "^1.6.12",
    "postgres": "^3.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-feather": "^2.0.10",
    "react-hook-form": "^7.52.2",
    "react-hot-toast": "^2.4.1",
    "react-twc": "^1.4.1",
    "resend": "^3.4.0",
    "socket.io": "^4.7.5",
    "tailwind-merge": "^2.4.0",
    "zustand": "^4.5.4"
  }
}